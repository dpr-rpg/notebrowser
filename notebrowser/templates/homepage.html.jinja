{% extends "page.html.jinja" %}
{% block title %}{{ site_data.config.title }}{% endblock %}
{% block heading %}<h1>{{ site_data.config.title }}</h1>{% endblock %}
{% block body %}
    <h2>Records</h2>
    <ul>
    {% for uri, record in site_data.records.items() %}
        <li>
            {{ uri }}{% if record.tagline %}: {{ record.tagline }}{% endif %}
        </li>
    {% endfor %}
    </ul>
{% endblock %}
